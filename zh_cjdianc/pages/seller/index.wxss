.canvas{
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10;
  background-color: rgba(0, 0, 0, .0);
  transition: background-color 500ms;
}
.canvas.active{
  background-color: rgba(0, 0, 0, .6);
}
.canvas .son{
  position: fixed;
  left: 0rpx;
  bottom:115rpx;
  width: 100vw;
  height: 30vh;
  background-color: white;
}
@keyframes upp{
  0%{transform: translateY(100%)}
  100%{transform: translateY(0)}
  
}
@keyframes downn{
  0%{transform: translateY(0)}
  100%{transform: translateY(100%)}
}
.canvas .son.upp{
  animation: upp 500ms;
}
.canvas .son.downn{
  animation: downn 500ms;
}
