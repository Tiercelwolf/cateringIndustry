page {
  background-color: white;
}

.one {
  margin-top:15rpx;
  height: 50rpx;
  width: 90vw;
  background-color:rgba(238, 238, 238, 0.534);
  border-radius:20rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
}

.one button{
  margin-right:2rpx;
  color: #ffbb89;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.one i {
  margin-right: 20rpx;
}

.list {
    background: #fff;
    padding: 30rpx;
    box-sizing: border-box;
    width: 100%;
    margin: 0 auto 0;
    color: #333;
    border-bottom: 1rpx solid #e5e5e5;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.list-left {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
}

.list-left1 {
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.list-left image {
    width: 180rpx;
    height: 120rpx;
    margin-right: 30rpx;
}

.list-left view:nth-child(1) {
    color: #333;
}

.list-left view:nth-child(2) {
    margin-top: 5rpx;
    color: #999;
    font-size: 25rpx;
}

.bottom {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.bottom i{
  color: white;
  background-color: #ffbb89;
  border-radius: 50%;
}

.bottom text {
  text-align: center;
  display: inline-block;
  width: 50rpx;
}

.car {
  width: 36px;
  height: 36px;
  background-color: rgba(155, 155, 155, 0.87);
  border-radius: 50%;
  position: fixed;
  z-index: 100000;
  top: 700rpx;
  left: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;                          
}

.car view {
  width: 30rpx;
  height: 30rpx;
  font-size: 20rpx;
  margin-left: -5rpx;
  margin-top: -10rpx;
  background-color: #ffbb89;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  color:white;
}

.search_no{
  width: 100%;
  height: 100%;
}

.search_no1{
  margin-top:360rpx;
  width: 100%;
  display: flex; 
  align-items: center;
  justify-content: center;
}

.search_no2{
  margin-top:10rpx;
  width: 100%;
  display: flex; 
  align-items: center;
  justify-content: center;
}