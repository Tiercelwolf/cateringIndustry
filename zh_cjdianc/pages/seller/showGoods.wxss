.gif{
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.gif image{
  width: 100%;
  height: 70%;
}

page {
  background-color: white;
}

.one {
  height: 50rpx;
  width: 90vw;
  background-color:rgba(238, 238, 238, 0.534);
  border-radius:20rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between; 
  align-items: center;
  padding-left: 20rpx;
}

.right{
  padding-bottom: 100rpx;
}

.two {
  margin-top: 20rpx;
  display: flex;
}
.two .left {
  width: 20vw;
  height: 90vh;
  background-color:rgba(238, 238, 238, 0.534);
}
.two .left .active{
  background-color: white;
  border-right: 0rpx solid white;
}
.two .right {
  width: 80vw;
  height: 90vh;
}
.two .left view {
  width: 20vw;
  height: 15vw;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(238, 238, 238, 0.0);
}
.two .right .top{
  padding: 20rpx 20rpx 0 20rpx;
  display: flex;
  align-items: center;
}
 .right .top image{
  width: 160rpx;
  height: 160rpx;
  margin-right: 20rpx; 
  margin-bottom:20rpx; 
  border-radius: 20rpx; 
}
.two .right .top>view view:nth-child(2){
  color:#ffbb89;
}
.two .right .top>view view:nth-child(3){
  color:#ccc;
  text-decoration-line: line-through;  
}
.two .right .bottom {
  margin-left: 260rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.two .right .bottom i{
  color: white;
  background-color: #ffbb89;
  border-radius: 50%;
}
.two .right .bottom text {
  text-align: center;
  display: inline-block;
  width: 50rpx;
}

.car {
  width: 36px;
  height: 36px;
  background-color: rgba(155, 155, 155, 0.87);
  border-radius: 50%;
  position: fixed;
  z-index: 100000;
  top: 700rpx;
  left: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;                          
}
.car view {
  width: 30rpx;
  height: 30rpx;
  font-size: 20rpx;
  margin-left: -5rpx;
  margin-top: -10rpx;
  background-color: #ffbb89;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  color:white;
}

.canshu{
  height: 116rpx;
  width: 100vw;
  color: white;
  font-size: 50rpx;
  background-color: #ffbb89;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
button.canshu {
  width: 100vw;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 40rpx;
  position: fixed;
  left: 0;
  bottom: 0;
}
