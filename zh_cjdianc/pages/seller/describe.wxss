.gif{
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.gif image{
  width: 100%;
  height: 70%;
}

.context image{
  width: 100%;
  height: 100%;
}
.context{
  height: 60vh;
}
.two{
  background-color: white;
  padding: 30rpx 0 10rpx 20rpx;
}
.two .top{
  display: flex;
  justify-content: space-between
}
.two .top .right{
  width: 200rpx;
  height: 100rpx;
  border-left: 10rpx dotted #ccc;
  display: flex;
  justify-content: center;
  align-items: center;
}
.two .top .right .temp{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
}

.two .top .left{
  font-size: 40rpx;
}
.two .top .left view:nth-child(2){
  color: #ffbb89;
}
.two   .bottom {
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  font-size:30rpx;
  color: #ccc;
}

.zhuangyuanfandian {
  background-color: white;
  padding: 30rpx 20rpx;
  display: flex;
  justify-content: space-between;
  margin: 20rpx 0;
}
.zhuangyuanfandian  text{
  color: #ffbb89;
}
.float{
  width: 92vw;
  height: 100rpx;
  /* margin:0 auto; */
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
  background-color: aliceblue; 
  position: fixed;
  left: 4vw;
  bottom: 2vh;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.float button{
  background-color: rgba(255, 255, 255, .1);
}

.float .right{
  width: 50vw;
  height: 60rpx;
  border-radius: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden; 
}
.float .right view:nth-child(1){
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color:#009ACD;
  color: white; 
}
.float .right view:nth-child(2){
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  color:  #009ACD;
}

.final {
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: white;
}

.final>view{
  width: 50%;
  border-bottom: 1rpx solid white;
  display: flex;
  justify-content: center;
  align-items: center;
}

.final view:nth-child(1).active{
  border-bottom: 1rpx solid #ffbb89;
  color: #ffbb89;
}

.img{
  display: flex;
  flex-direction: column;
}

.image{  
  width: 100%;
}

.pinglun {
  border-top: 1rpx solid #ccc;
  padding: 20rpx 16rpx;
  display: flex;
  justify-content: space-between
}
.pinglun image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.pinglun >view:nth-child(1){
  display: flex;
  align-items: flex-start;
}
.save{
  width: 40rpx;
  height: 40rpx;
}