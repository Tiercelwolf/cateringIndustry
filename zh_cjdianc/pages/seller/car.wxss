page {
  background-color: white;
}

.one {
  display: flex;
}

.one .left,.one .right{
  width: 50vw;
  height: 8vh;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 4rpx solid #ccc;
  background-color: rgb(240, 240, 240); 
}

.one .active {
  background-color: white;
  border-bottom: 4rpx solid #ffbb89;
}

.bottom {
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  padding-bottom: 110rpx;
}

.bottom .del{
  border-top:1rpx solid rgb(236, 234, 234);
  border-bottom:1rpx solid rgb(240, 240, 240); 
  padding: 0 3vw;
  width: 100%;
  height: 6vh;
  background-color: white;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.bottom .pay{
  width: 100%;
  height: 8vh;
  display: flex;
}

.bottom .del view{
  font-size: 20rpx;
  letter-spacing:1rpx;
}

.bottom .del view:nth-child(2){
  width: 20vw;
  height: 4vh;
  color: #ffbb89;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
}

.bottom .del view:nth-child(3){
  width: 20vw;
  height: 4vh;
  color: #ffbb89;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
}

.bottom .pay .left{
  padding: 0 2vw;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 70vw;
}

.bottom .pay .right{
  width: 30vw;
  background-color: #ffbb89;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
}

.bottom .pay .righttt{
  width: 30vw;
  background-color: #ccc;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
}

.bottom .pay .left>view:nth-child(2){
  color: #ffbb89;
  font-size: 30rpx;
}

.bottom .pay .left>view:nth-child(2){
  color: #ffbb89;
  font-size: 30rpx;
}

.bottom .pay .left>view i {
  margin-right: 10rpx;
}

.bottom .pay .left>view text {
  letter-spacing: 1rpx;
}

.show {
  height: 76vh;
  background-color: white;
  position: relative;
}

.show .item{
  background-color: skyblue;
  margin: 20rpx 0 0 0;
  height: 200rpx;
  width: 100%;
}

.show .left{
  background-color: rgb(240, 240, 240);
  padding: 20rpx;
  position: relative;
  width: 100%;
  height: 200rpx;
  display: flex;
  align-items: center;
}

.show .left image{
  width: 150rpx;
  height: 150rpx;
  margin: 20rpx;
}

.show .left .shu view:nth-child(1){
  margin-bottom: 72rpx;
}

.show .left>.jj{
  position: absolute;
  bottom: 0;
  right: 50rpx;
}

.show .left>.cha{
  position: absolute;
  top: 0;
  right: 50rpx;
  color: #ccc;
}

.show .left>.jj i{
  background-color: #ffbb89;
  border-radius: 50%;
  color:white;  
}

.show .left>.jj text{
  display: inline-block;
  width: 50rpx;
  text-align: center;
}
