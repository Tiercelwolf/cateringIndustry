.collections {
    background: #fff;
    padding: 30rpx;
    box-sizing: border-box;
    width: 100%;
    font-family: 微软雅黑;
    margin: 0 auto 0;
    font-size: 30rpx;
    color: #333;
    border-bottom: 1rpx solid #e5e5e5;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.collections-left {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
}

.collections-left1 {
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.collections-left image {
    width: 150rpx;
    height: 150rpx;
    margin-right: 30rpx;
     
}

.collections-left view:nth-child(1) {
    color: #333;
}

.collections-left view:nth-child(2) {
    margin-top: 5rpx;
    color: #999;
    font-size: 25rpx;
}

.list_imgbk {
    width: 300rpx;
    height: 240rpx;
    margin-bottom: 30rpx;
}

.list_imgbk2 {
    box-sizing: content-box;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    flex-direction: column;
    height: 500rpx;
}

.list_imgbk3 {
    font-size: 30rpx;
    color: #999;
    text-align: center;
    line-height: 30rpx;
}

.bottom {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.bottom i{
  color: white;
  background-color: #ffbb89;
  border-radius: 50%;
}

.bottom text {
  text-align: center;
  display: inline-block;
  width: 50rpx;
}

.car {
  width: 36px;
  height: 36px;
  background-color: rgba(155, 155, 155, 0.87);
  border-radius: 50%;
  position: fixed;
  z-index: 100000;
  top: 700rpx;
  left: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;                          
}

.car view {
  width: 30rpx;
  height: 30rpx;
  font-size: 20rpx;
  margin-left: -5rpx;
  margin-top: -10rpx;
  background-color: #ffbb89;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  color:white;
}

